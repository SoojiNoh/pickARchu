<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.soojinoh.pickarchu.MainActivity">

    <TextView
        android:id="@+id/textView_capture_save_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView_call_gallery_test"
        android:layout_alignParentTop="true"
        android:layout_marginTop="16dp"
        android:onClick="onCameraViewClick"
        android:text="capture_save_test" />

    <TextView
        android:id="@+id/textView_call_gallery_test"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="216dp"
        android:onClick="onMoreImageViewClick"
        android:text="call_gallery_test" />

</RelativeLayout>